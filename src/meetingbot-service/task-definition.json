{
  "family": "meetingbot-dev-meet-bot-local",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "4096",
  "memory": "16384",
  "executionRoleArn": "arn:aws:iam::228692667167:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::228692667167:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "bot",
      "image": "228692667167.dkr.ecr.us-east-1.amazonaws.com/meetingbot-service:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/meetingbot-dev-meet-bot-local",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ]
    }
  ]
}
